	do_background() {

	build_dir=$(mktemp -d -t build_XXXXXXXX -p $build_dir_root)
	cd $build_dir


	awk -v s="drm_dp_start_crc drm_dp_stop_crc" '
		BEGIN {
			n = 0;
			num = split(s, symbols, " ")
		} {
			for (i in symbols)
				if (symbols[i] == $2)
					n++
		} END {
			if (num == n)
				exit 0;
			else
				exit 1
		}' $LINUX_OBJ/$LINUX_SYMBOLS 2>/dev/null
	rc=$?
	if test $rc -ne 0; then
		n=0
		export=0
		for file in drivers/gpu/drm/drm_dp_helper.c; do
			n=$(awk -v s="drm_dp_start_crc drm_dp_stop_crc" '
				BEGIN {
					n = 0;
					split(s, symbols, " ")
				} {
					for (i in symbols) {
						s="EXPORT_SYMBOL.*\\("symbols[i]"\\);"
						if ($0 ~ s)
							n++
					}
				} END {
					print n
				}' $LINUX/$file 2>/dev/null)
			rc=$?
			if test $rc -eq 0; then
				export=$(( $export+$n ))
			fi
		done
		if test $(echo "drm_dp_start_crc drm_dp_stop_crc" | wc -w) -eq $export; then :

printf "%s\n" "#define HAVE_DRM_DP_START_CRC 1" >>confdefs.h


		else :

		fi
	else :

printf "%s\n" "#define HAVE_DRM_DP_START_CRC 1" >>confdefs.h


	fi


	if test -s confdefs.h
then :

		cat confdefs.h >>$build_dir_root/confdefs.h

fi
	cd $build_dir_root
	rm -rf $build_dir

	}

