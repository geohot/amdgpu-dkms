		} {
			for (i in symbols)
				if (symbols[i] == $2)
					n++
		} END {
			if (num == n)
				exit 0;
			else
				exit 1
		}' $LINUX_OBJ/$LINUX_SYMBOLS 2>/dev/null
	rc=$?
	if test $rc -ne 0; then
		n=0
		export=0
		for file in drivers/gpu/drm/drm_drv.c; do
			n=$(awk -v s="drm_dev_enter" '
				BEGIN {
					n = 0;
					split(s, symbols, " ")
				} {
					for (i in symbols) {
						s="EXPORT_SYMBOL.*\\("symbols[i]"\\);"
						if ($0 ~ s)
							n++
					}
				} END {
					print n
				}' $LINUX/$file 2>/dev/null)
			rc=$?
			if test $rc -eq 0; then
				export=$(( $export+$n ))
			fi
		done
		if test $(echo "drm_dev_enter" | wc -w) -eq $export; then :
			rc=0
		else :
			rc=1
		fi
	else :
		rc=0
	fi

