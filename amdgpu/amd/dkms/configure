  IFS=$as_save_IFS
  case $as_dir in #(((
    '') as_dir=./ ;;
    */) ;;
    *) as_dir=$as_dir/ ;;
  esac
    for ac_exec_ext in '' $ac_executable_extensions; do
  if as_fn_executable_p "$as_dir$ac_word$ac_exec_ext"; then
    ac_cv_prog_NPROC="yes"
    printf "%s\n" "$as_me:${as_lineno-$LINENO}: found $as_dir$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

fi
fi
NPROC=$ac_cv_prog_NPROC
if test -n "$NPROC"; then
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $NPROC" >&5
printf "%s\n" "$NPROC" >&6; }
else
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
fi


	if test x"$NPROC" != x"yes"
then :

		ncpu=1

else $as_nop

		ncpu=$(nproc)

fi

	while [ $(jobs | wc -l) -gt $ncpu ]
	do
		sleep 0.1
	done

	do_background &
	procs="$! $procs"




	do_background() {

	build_dir=$(mktemp -d -t build_XXXXXXXX -p $build_dir_root)
	cd $build_dir



