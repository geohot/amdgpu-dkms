	do_background() {

	build_dir=$(mktemp -d -t build_XXXXXXXX -p $build_dir_root)
	cd $build_dir




cat $build_dir_root/confdefs.h - <<_ACEOF >conftest.c


			#include <linux/kallsyms.h>

int
main (void)
{

			kallsyms_lookup_name(NULL);

  ;
  return 0;
}

_ACEOF



cat - <<_ACEOF >conftest.h

_ACEOF


	if { ac_try='eval $CC $CFLAGS'
  { { eval echo "\"\$as_me\":${as_lineno-$LINENO}: \"$ac_try\""; } >&5
  (eval $ac_try) 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; } > /dev/null && { ac_try='test -s conftest.o || test -s .tmp_conftest.o'
  { { eval echo "\"\$as_me\":${as_lineno-$LINENO}: \"$ac_try\""; } >&5
  (eval $ac_try) 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; }
then :
  rc=0
else $as_nop
  printf "%s\n" "$as_me: failed program was:" >&5
sed 's/^/| /' conftest.$ac_ext >&5
 rc=1


fi


	if test $rc -ne 0; then :

	else

	awk -v s="kallsyms_lookup_name" '
		BEGIN {
			n = 0;
			num = split(s, symbols, " ")
		} {
			for (i in symbols)
				if (symbols[i] == $2)
					n++
		} END {
			if (num == n)
				exit 0;
			else
				exit 1
		}' $LINUX_OBJ/$LINUX_SYMBOLS 2>/dev/null
	rc=$?
	if test $rc -ne 0; then
		n=0
		export=0
		for file in kernel/kallsyms.c; do
			n=$(awk -v s="kallsyms_lookup_name" '
				BEGIN {
					n = 0;
					split(s, symbols, " ")
				} {
					for (i in symbols) {
						s="EXPORT_SYMBOL.*\\("symbols[i]"\\)"
						if ($0 ~ s)
							n++
					}
				} END {
					print n
				}' $LINUX/$file 2>/dev/null)
			rc=$?
			if test $rc -eq 0; then
				export=$(( $export+$n ))
			fi
		done
		if test $(echo "kallsyms_lookup_name" | wc -w) -eq $export; then :
			rc=0
		else :
			rc=1
		fi
	else :
		rc=0
	fi

		if test $rc -ne 0; then :

		else :


printf "%s\n" "#define HAVE_KALLSYMS_LOOKUP_NAME 1" >>confdefs.h


		fi
	fi


	if test -s confdefs.h
then :

		cat confdefs.h >>$build_dir_root/confdefs.h

fi
	cd $build_dir_root
	rm -rf $build_dir

	}

	# Extract the first word of "nproc", so it can be a program name with args.
set dummy nproc; ac_word=$2
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
printf %s "checking for $ac_word... " >&6; }
if test ${ac_cv_prog_NPROC+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  if test -n "$NPROC"; then
  ac_cv_prog_NPROC="$NPROC" # Let the user override the test.
else
as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  case $as_dir in #(((
    '') as_dir=./ ;;
    */) ;;
    *) as_dir=$as_dir/ ;;
  esac
    for ac_exec_ext in '' $ac_executable_extensions; do
  if as_fn_executable_p "$as_dir$ac_word$ac_exec_ext"; then
    ac_cv_prog_NPROC="yes"
    printf "%s\n" "$as_me:${as_lineno-$LINENO}: found $as_dir$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

fi
fi
NPROC=$ac_cv_prog_NPROC
if test -n "$NPROC"; then
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $NPROC" >&5
printf "%s\n" "$NPROC" >&6; }
else
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
fi


	if test x"$NPROC" != x"yes"
then :

		ncpu=1

else $as_nop

		ncpu=$(nproc)

fi

	while [ $(jobs | wc -l) -gt $ncpu ]
	do
		sleep 0.1
	done

	do_background &
	procs="$! $procs"



	{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for supported chips" >&5
printf %s "checking for supported chips... " >&6; }
	if test $HAVE_DRM_AMD_ASIC_TYPE_H
then :

		chips=$(awk 'BEGIN {enum = 0} {
			if ($0 ~ "^enum amd_asic_type")
				enum = 1;
			if (enum && $1 ~ "CHIP_") {
				gsub(",", "");
				if ($1 == "CHIP_LAST")
					exit;
				print $1;
			}
		}' ../../include/drm/amd_asic_type.h)

		for i in $chips; do
			$as_echo "#define HAVE_$i" >>config/config-amd-chips.h
		done
		{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: done" >&5
printf "%s\n" "done" >&6; }

else $as_nop

		{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: failed" >&5
printf "%s\n" "failed" >&6; }

fi




	do_background() {

	build_dir=$(mktemp -d -t build_XXXXXXXX -p $build_dir_root)
	cd $build_dir



cat $build_dir_root/confdefs.h - <<_ACEOF >conftest.c


			#include <linux/idr.h>

int
main (void)
{

			void *i;
			i = idr_remove(NULL, 0);

  ;
  return 0;
}

_ACEOF



cat - <<_ACEOF >conftest.h

_ACEOF


	if { ac_try='eval $CC $CFLAGS'
  { { eval echo "\"\$as_me\":${as_lineno-$LINENO}: \"$ac_try\""; } >&5
  (eval $ac_try) 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; } > /dev/null && { ac_try='test -s conftest.o || test -s .tmp_conftest.o'
