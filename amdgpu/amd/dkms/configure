printf "%s\n" "#define HAVE_MEMALLOC_NORECLAIM_SAVE 1" >>confdefs.h


else $as_nop
  printf "%s\n" "$as_me: failed program was:" >&5
sed 's/^/| /' conftest.$ac_ext >&5


fi



