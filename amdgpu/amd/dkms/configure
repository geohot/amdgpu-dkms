	do_background() {

	build_dir=$(mktemp -d -t build_XXXXXXXX -p $build_dir_root)
	cd $build_dir


	awk -v s="mmput_async" '
		BEGIN {
			n = 0;
			num = split(s, symbols, " ")
		} {
			for (i in symbols)
				if (symbols[i] == $2)
					n++
		} END {
			if (num == n)
				exit 0;
			else
				exit 1
		}' $LINUX_OBJ/$LINUX_SYMBOLS 2>/dev/null
	rc=$?
	if test $rc -ne 0; then
		n=0
		export=0
		for file in kernel/fork.c; do
			n=$(awk -v s="mmput_async" '
				BEGIN {
					n = 0;
					split(s, symbols, " ")
				} {
					for (i in symbols) {
						s="EXPORT_SYMBOL.*\\("symbols[i]"\\);"
						if ($0 ~ s)
							n++
					}
				} END {
					print n
				}' $LINUX/$file 2>/dev/null)
			rc=$?
			if test $rc -eq 0; then
				export=$(( $export+$n ))
			fi
		done
		if test $(echo "mmput_async" | wc -w) -eq $export; then :


printf "%s\n" "#define HAVE_MMPUT_ASYNC 1" >>confdefs.h


		else :

		fi
	else :


printf "%s\n" "#define HAVE_MMPUT_ASYNC 1" >>confdefs.h


	fi


	if test -s confdefs.h
then :

		cat confdefs.h >>$build_dir_root/confdefs.h

fi
	cd $build_dir_root
	rm -rf $build_dir

	}

	# Extract the first word of "nproc", so it can be a program name with args.
